[Unit]
Description=IRIS Advanced Traffic Management System (ATMS)
After=postgresql.service

[Service]
ExecStart=/usr/bin/java -server -Xmx512m -agentlib:jdwp=transport=dt_socket,server=y,suspend=n -jar /usr/share/java/iris-server/iris-server.jar
Type=simple
Restart=always
User=tms
StandardOutput=null
StandardError=null

[Install]
WantedBy=multi-user.target
